{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": [
    "_key",
    "workspace_id",
    "owner"
  ],
  "properties": {
    "_key": {
      "type": "string",
      "description": "The workspace reference for this object",
      "examples": [ "1:2:3" ]
    },
    "workspace_id": {
      "type": "integer",
      "description": "The workspace_id for this object",
      "minimum": 1
    },
    "owner": {
      "type": "string",
      "description": "The owner of this workspace object"
    }
  }
}
